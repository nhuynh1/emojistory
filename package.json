{
  "name": "giraffe",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "browser-sync start --server 'public' --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --port 7777 --browser 'Firefox Developer Edition'",
    "build:site": "gulp build",
    "build:functions": "netlify-lambda build src/netlify-functions",
    "serve:site": "http-server ./public --proxy http://localhost:9000 -c-1",
    "serve:functions": "netlify-lambda serve src/netlify-functions",
    "serve:browsersync": "gulp serve",
    "build": "npm-run-all --parallel build:**",
    "serve": "npm-run-all --parallel serve:**"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.10.1",
    "autoprefixer": "^9.8.0",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.12",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^3.1.1"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
